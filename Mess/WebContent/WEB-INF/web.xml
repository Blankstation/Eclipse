	</servlet>
	<servlet-mapping>
		<servlet-name>FetchMessageServlet</servlet-name>
		<url-pattern>/FetchMessageServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>messageServlet</display-name>
		<servlet-name>messageServlet</servlet-name>
		<servlet-class>com.servlet.messageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>messageServlet</servlet-name>
		<url-pattern>/messageServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>DelMsgServlet</display-name>
		<servlet-name>DelMsgServlet</servlet-name>
		<servlet-class>com.servlet.DelMsgServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DelMsgServlet</servlet-name>
		<url-pattern>/DelMsgServlet</url-pattern>
	</servlet-mapping>

</web-app>


